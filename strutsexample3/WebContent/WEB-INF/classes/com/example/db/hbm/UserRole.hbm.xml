<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.example.db">
	<class name="UserRole" table="user_role">
		<id name="user_role" type="java.lang.Integer" column="user_role"><generator class="identity"/></id>
		<property name="userid" column="userid" type="string" not-null="true" length="12"/>	
		
		<!--  <many-to-one name= "user" class ="User"  column="userid1" not-null="true"/>-->
		
	</class>
</hibernate-mapping>